-color: #fff !important;\n}\n.hero .container {\n  color: #333;\n}\n.hero.var-height {\n  height: auto !important;\n}\n\n/*Buttons*/\n.btn-light {\n  box-shadow: 0px 0px 10px 4px #e1e1e1;\n  -webkit-box-shadow: 0px 0px 10px 4px #e1e1e1;\n  -moz-box-shadow: 0px 0px 10px 4px #e1e1e1;\n  transition: box-shadow 0.2s ease-in !important;\n}\n.btn-light:hover {\n  box-shadow: none;\n}\n\n/*Navigation Bar*/\n.sticky-top {\n  box-shadow: 0px 0px 15px 0px #e1e1e1;\n  -webkitbox-shadow: 0px 0px 15px 0px #e1e1e1;\n  -moz-box-shadow: 0px 0px 15px 0px #e1e1e1;\n}\n\n/*Landing Page*/\n.home {\n  background: url(../img/Blocks.png);\n  background-size: 500px;\n  background-repeat: no-repeat;\n  background-position: center right;\n}\n\n/*Join Page*/\n.join img {\n  max-width: 400px;\n  border-radius: 100%;\n  transition: box-shadow 0.3s ease-in;\n  box-shadow: 0px 0px 22px 20px #e1e1e1;\n  -webkit-box-shadow: 0px 0px 22px 20px #e1e1e1;\n  -moz-box-shadow: 0px 0px 22px 20px #e1e1e1;\n}\n.join img:hover {\n  box-shadow: 0px 0px 22px 0px #e1e1e1;\n  -webkit-box-shadow: 0px 0px 22px 0px #e1e1e1;\n  -moz-box-shadow: 0px 0px 22px 0px #e1e1e1;\n}\n\n/*Tokens*/\n.form-body {\n  padding: 20px;\n  box-shadow: 0px 0px 22px 0px #e1e1e1;\n  -webkitbox-shadow: 0px 0px 22px 0px #e1e1e1;\n  -moz-box-shadow: 0px 0px 22px 0px #e1e1e1;\n}\n.tokens img {\n  max-width: 300px;\n  margin: auto;\n}\n\n/*Process*/\n.process {\n  text-align: center;\n}\n.visor {\n  display: none !important;\n}\n.spinner-border.text-primary {\n  width: 8em;\n  height: 8em;\n  margin-bottom: 50px;\n}\n\n/*Submit*/\n.submit .custom-file {\n  margin-bottom: 25px !important;\n}\n.submit img {\n  max-width: 400px;\n}\n\n/*Thank You*/\n.thank-you img {\n  max-width: 400px;\n}\n/*Media Queries*/\n@media only screen and (min-width: 981px) {\n  .navbar-brand {\n    margin-right: 4rem !important;\n  }\n}\n\n@media only screen and (max-width: 480px) {\n  .navbar-brand {\n    font-size: 1rem !important;\n  }\n  .display-3 {\n    font-size: 2.5rem !important;\n  }\n  .home {\n    background-size: 200px;\n    background-position: top;\n    margin-top: 10px;\n  }\n  .home .col {\n    margin-top: 60px;\n  }\n  #align-not {\n    align-items: start !important;\n  }\n}\n"},"lineCount":null}},"error":null,"hash":"2c491e1310cb8aaba3fede8ee5391878","cacheData":{"env":{}}}